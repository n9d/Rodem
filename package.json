{
	"name": "rodem",
	"displayName": "rodem",
	"description": "",
	"publisher": "n9d",
	"repository": "https://github.com/n9d/Rodem",
	"version": "0.1.10",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rodem.executecode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rodem.executecode",
				"title": "Rodem: Execute Code"
			}
		],
		"keybindings": [
			{
				"command": "rodem.executecode",
				"key": "F5",
				"when": "resourceExtname == .md"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Rodem",
			"properties": {
				"rodem.lang": {
					"type": "object",
					"default": {
						"js": "node",
						"ts": "ts-node",
						"rb": "ruby",
						"sh": "bash",
						"py": "python",
						"rails": "rails console",
						"django": "python manage.py shell",
						"django+": "python manage.py shell_plus",
						"django++": "python manage.py shell_plus --print-sql"
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"temp": "^0.9.4"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/temp": "^0.9.0",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"lodash": "^4.17.21",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vsce": "^1.88.0",
		"vscode-test": "^1.5.0"
	}
}
